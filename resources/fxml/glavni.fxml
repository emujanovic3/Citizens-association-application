<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <GridPane hgap="10.0" minHeight="-Infinity" minWidth="-Infinity" vgap="10.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="15.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="30.0" prefHeight="100.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="nazivLabel" text="Naziv" GridPane.columnIndex="2">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label text="Udruženje građana:" GridPane.columnIndex="1">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label text="Članovi:" GridPane.rowIndex="1" />
            <TableView fx:id="clanoviTableView" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
              <columns>
                <TableColumn fx:id="imeCol" prefWidth="90.0" text="Ime" />
                <TableColumn fx:id="prezimeCol" prefWidth="100.0" text="Prezime" />
              </columns>
            </TableView>
            <VBox prefWidth="100.0" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.rowSpan="2">
               <children>
                  <Button mnemonicParsing="false" onAction="#dodajClanaAction" prefWidth="100.0" text="Dodaj člana" />
                  <Button mnemonicParsing="false" onAction="#obrisiClanaAction" prefWidth="100.0" text="Obriši člana" />
                  <Button mnemonicParsing="false" onAction="#skupstinaAction" prefWidth="120.0" text="Skupština" />
                  <Button mnemonicParsing="false" onAction="#dodajSkupstinaAction" prefWidth="120.0" text="Dodaj u skupštinu" />
                  <Button mnemonicParsing="false" onAction="#izbaciSkupstinaAction" prefWidth="120.0" text="Izbaci iz skupštine" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </GridPane>
   </center>
</BorderPane>
